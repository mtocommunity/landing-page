import type { EventAgenda } from "../../../types";
import AgendaCard from "../molecule/AgendaCard.astro";

type Props = {
  className?: string;
  agenda: EventAgenda;
};

const { agenda, className } = Astro.props;
---

<>
  {
    agenda.days.map((day) => (
      <AgendaCard className={className} agendaDay={day} />
    ))
  }
</>
