---
import Layout from "../layouts/Layout.astro";
import { MTOTransparentBackground } from "../assets";

import Social from "../components/general/molecule/social";
---

<Layout title="ManyToOne Community">
  <main class="flex justify-center items-center h-screen bg-mto_dark_gray">
    <div class="flex flex-col text-white relative">
      <div class="flex flex-col items-center">
        <MTOTransparentBackground className="w-64" />
        <h1 class="text-2xl font-bold">ManyToOne</h1>
        <h2 class="flex">Community</h2>
        <Social />
      </div>
    </div>
  </main>
</Layout>
